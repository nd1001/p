<script>
var map=L.map('map',{scrollWheelZoom:false}).setView([0.7119444,37.9616666], 7);

   L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', { 
   attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors' 
   }).addTo(map);
   
 
   
   
    var marker = L.marker([-1.280000,36.806944]).addTo(map);

</script>
